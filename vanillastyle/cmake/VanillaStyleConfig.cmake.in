@PACKAGE_INIT@

find_dependency(QT NAMES Qt6 Qt5 COMPONENTS Core Gui Widgets REQUIRED)
find_dependency(Qt${QT_VERSION_MAJOR} COMPONENTS Core Gui Widgets REQUIRED)

include("${CMAKE_CURRENT_LIST_DIR}/VanillaStyleTargets.cmake")

set_and_check(VanillaStyle_INCLUDE_DIR "@PACKAGE_VanillaStyle_INCLUDE_DIR@")
set(VanillaStyle_JSON_INCLUDE_DIR "@PACKAGE_JSON_INCLUDE_DIR@")

# Add the include dirs to the imported target
set_target_properties(VanillaStyle::VanillaStyle PROPERTIES
    INTERFACE_INCLUDE_DIRECTORIES "${VanillaStyle_INCLUDE_DIR};${VanillaStyle_JSON_INCLUDE_DIR}"
)

check_required_components(VanillaStyle)
